@PACKAGE_INIT@

# resolve dependencies
include(${CMAKE_CURRENT_LIST_DIR}/erl_before_find_package.cmake)
include(${CMAKE_CURRENT_LIST_DIR}/erl_find_package.cmake)
foreach(package erl_common)
    if (NOT TARGET ${package}::${package})
        erl_find_package(
                PACKAGE_NAME erl_common
                NO_CMD "git clone https://github.com/ExistentialRobotics/erl_common.git"
                FIND_PACKAGE_ARGS REQUIRED)
    endif()
endforeach()

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

# name alias
# add_library(@PROJECT_NAME@::pybind ALIAS @PROJECT_NAME@::@PROJECT_NAME@_pybind)

check_required_components("@PROJECT_NAME@")

# export variables
set(@PROJECT_NAME@_FOUND
    TRUE
    CACHE INTERNAL "TRUE if @PROJECT_NAME@ and all required components found on the system" FORCE)
message(STATUS "@PROJECT_NAME@ is found")
